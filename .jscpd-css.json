{
	"": "https://www.npmjs.com/package/jscpd#options",
	"threshold": 20.1,
	"ignore": [
		"**/__snapshots__/**",
		"**/node_modules/**",
		"**/*.min.js",
		"**/*.map",
		"**/dist/**"
	],
	"absolute": true,
	"pattern": "./src/**/*.{scss,less}",
	"min-tokens": 50,
	"min-lines": 5,
	"max-lines": 1000,
	"max-size": "100kb",
	"reporters": [
        "consoleFull", 
        "markdown", 
        "html"
    ],
    "output": "./log/jscpd-css",
    "mode": "mild",
    "format": [
        "css",
        "scss",
        "less"
    ],
    "blame": false,
    "ignoreCase": true,
    "noSymLinks": true
}
