		"stylelint-processor-styled-components": "^1.10.0",
		"stylelint-scss": "^3.17.2",
		"ts-node": "^9.0.0",
		"tslib": "^2.0.1",
		"tslint": "^6.1.3",
		"tslint-angular": "^3.0.3",
		"typescript": "^4.0.3"
	}
}
