{
	"name": "lint",
	"version": "1.1.0",
	"description": "",
	"private": "false",
	"main": "src/index.ts",
	"engines": {
		"node": ">=14.5.0"
	},
	"scripts": {
		"!!!for global linting!!!": "export LINTER_PATH=./",
		"ngng": "node --optimize_for_size --max_old_space_size=4000 --gc_interval=100 ./node_modules/@angular/cli/bin/ng",
		"lint:all": "npm run lint:ts; npm run lint:es; npm run lint:ts; npm run lint:scss; npm run lint:spell; npm run lint:cp",
		"lint:ts": "tslint -c ${LINTER_PATH}tslint.json -p ./tsconfig.eslint.json",
		"lint:es": "eslint -c ${LINTER_PATH}.eslintrc.js ./src",
		"lint:scss": "stylelint --config ${LINTER_PATH}.stylelintrc.json --syntax scss \"./**/*.scss\"",
		"lint:spell": "cspell -c ${LINTER_PATH}/.cspell.json '**/*.{js,ts}'",
		"lint:cp": "jscpd -c .jscpd.json ."
	},
	"author": "stepanovv.ru",
	"license": "MIT",
	"dependencies": {
		"@angular/compiler": "^10.1.3",
		"@angular/core": "^10.1.3",
		"rxjs": "^6.6.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1001.2",
		"@angular-eslint/builder": "0.0.1-alpha.32",
		"@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
		"@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
		"@angular-eslint/schematics": "0.0.1-alpha.32",
		"@angular-eslint/template-parser": "0.0.1-alpha.32",
		"@angular/cli": "^10.1.2",
		"@angular/compiler-cli": "^10.1.2",
		"@angular/language-service": "^10.1.2",
		"@types/fingerprintjs2": "^2.0.0",
		"@types/jasmine": "^3.5.14",
		"@types/jasminewd2": "~2.0.3",
		"@types/lodash": "^4.14.170",
		"@types/node": "^15.6.0",
		"@typescript-eslint/eslint-plugin": "^4.24.0",
		"@typescript-eslint/parser": "^4.24.0",
		"codelyzer": "^6.0.2",
		"cspell": "^5.4.1",
		"doiuse": "^4.4.1",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-import-resolver-typescript": "^2.4.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-compat": "^3.9.0",
		"eslint-plugin-flowtype": "^5.7.2",
		"eslint-plugin-import": "^2.23.3",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-rxjs": "^3.3.1",
		"eslint-plugin-standard": "^5.0.0",
		"jscpd": "^3.3.26",
		"jscpd-badge-reporter": "^1.1.3",
		"node-sass": "^6.0.0",
		"npm-run-all": "^4.1.5",
