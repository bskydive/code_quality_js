{
	"name": "lint",
	"version": "1.1.0",
	"description": "",
	"private": "false",
	"main": "src/index.ts",
	"engines": {
		"node": ">=14.5.0"
	},
	"scripts": {
		"!!!for global linting!!!": "export LINTER_PATH=./",
		"ngng": "node --optimize_for_size --max_old_space_size=4000 --gc_interval=100 ./node_modules/@angular/cli/bin/ng",
		"lint:all": "npm-run-all npm run ngng lint:es && npm run ngng lint:ts && npm run lint:scss",
		"lint:ts": "tslint -c ${LINTER_PATH}tslint.json -p ./tsconfig.eslint.json",
		"lint:es": "eslint -c ${LINTER_PATH}.eslintrc.js ./src",
		"lint:scss": "stylelint --config ${LINTER_PATH}.stylelintrc.json --syntax scss \"./**/*.scss\"",
		"lint:spell": "cspell -c ${LINTER_PATH}/.cspell.json '**/*.{js,ts}'",
		"lint:cp": "jscpd -c .jscpd.json ."
	},
	"author": "stepanovv.ru",
	"license": "MIT",
	"dependencies": {
		"@angular/compiler": "^10.1.3",
		"@angular/core": "^10.1.3",
		"rxjs": "^6.6.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.1001.2",
		"@angular-eslint/builder": "0.0.1-alpha.32",
		"@angular-eslint/eslint-plugin": "0.0.1-alpha.32",
		"@angular-eslint/eslint-plugin-template": "0.0.1-alpha.32",
		"@angular-eslint/schematics": "0.0.1-alpha.32",
		"@angular-eslint/template-parser": "0.0.1-alpha.32",
		"@angular/cli": "^10.1.2",
		"@angular/compiler-cli": "^10.1.2",
		"@angular/language-service": "^10.1.2",
		"@types/fingerprintjs2": "^2.0.0",
		"@types/jasmine": "^3.5.14",
		"@types/jasminewd2": "~2.0.3",
		"@types/lodash": "^4.14.161",
		"@types/node": "^14.11.1",
		"@typescript-eslint/eslint-plugin": "^4.2.0",
		"@typescript-eslint/parser": "^4.2.0",
		"codelyzer": "^6.0.0",
		"cspell": "^4.1.0",
		"doiuse": "^4.2.0",
		"eslint": "^7.9.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-standard": "^14.1.1",
		"eslint-import-resolver-typescript": "^2.3.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-compat": "^3.6.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.2",
		"eslint-plugin-rxjs": "0.0.2-beta.29",
		"eslint-plugin-standard": "^4.0.1",
		"jscpd": "^3.3.19",
		"jscpd-badge-reporter": "^1.1.3",
		"node-sass": "^4.14.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"rxjs-tslint-rules": "^4.34.3",
		"stylelint": "^13.7.1",
		"stylelint-config-recommended-scss": "^4.2.0",
		"stylelint-config-sass-guidelines": "^7.1.0",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-config-styled-components": "^0.1.1",
